{
  "name": "express-mysql",
  "version": "1.0.0",
  "description": "Small demo for using mysql with express and nodejs",
  "main": "dist/app.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "postbuild": "cp -r ./src/assets/ ./dist && cp -r ./src/views/ ./dist && cp ./src/.env ./dist",
    "prestart": "npm run build",
    "start": "node ./dist/app.js",
    "predev": "npm run build",
    "dev": "tsc --watch & nodemon ./dist/app.js"
  },
  "keywords": [],
  "author": "@Massiles GHERNAOUT || GITHUB/MassiGy",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.13",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "ejs-mate": "^4.0.0",
    "express": "^4.18.2",
    "mysql": "^2.18.1"
  }
}
