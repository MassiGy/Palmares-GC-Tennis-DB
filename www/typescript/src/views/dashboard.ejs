            <div class="card mb-4">
              <div class="card-header">
                <i class="fas fa-table me-1"></i>
                DataTable
              </div>
              <div class="card-body">
                <% if(payload.filled){ %>

                <table id="datatablesSimple">
                  <thead>
                    <tr>
                      <% for(let colomn of payload.colomns){ %>
                      <th><%= colomn %></th>
                      <% } %>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <% for(let colomn of payload.colomns){ %>
                      <th><%= colomn %></th>
                      <% } %>
                    </tr>
                  </tfoot>
                  <tbody>
                    <% for(let record of payload.data){ %>
                    <tr>
                      <% for(let val of Object.values(record)){ %>
                      <td><%= val %></td>
                      <% } %>
                    </tr>
                    <% } %>
                  </tbody>
                </table>
                <% } %>
              </div>
            </div>
